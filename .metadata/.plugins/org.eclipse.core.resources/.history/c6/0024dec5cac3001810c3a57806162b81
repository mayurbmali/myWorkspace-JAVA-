package lunchtime2018;

import java.util.Scanner;

public class judgintgDelay {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner scn = new Scanner(System.in);
		long x = scn.nextLong();
		int n = scn.nextInt();
		int count =0;
		for (int i = 0; i < n; i++) {
			int n1 = scn.nextInt();
			int n2 = scn.nextInt();
			if((n2-n1)>5) {
				count++;
			}
		}
		System.out.println(count);
	}

}
